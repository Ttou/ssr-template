{
  "name": "ssr-template",
  "author": "WangRuiJin <1361572192@qq.com>",
  "repository": "git@gitee.com:jh_shot/ssr-template.git",
  "license": "MulanPSL2",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "start": "node .output/server/index.mjs",
    "preview": "nuxt preview",
    "prepare": "husky install",
    "commit": "git add . && git cz",
    "format": "prettier \"src/**/*.{ts,tsx,vue}\" --write",
    "lint:script": "eslint \"src/**/*.{ts,tsx,vue}\" --fix",
    "lint:style": "stylelint \"src/**/*.{css,less,vue}\" --fix"
  },
  "dependencies": {
    "@pinia/nuxt": "0.1.9",
    "ant-design-vue": "3.2.5",
    "dayjs": "1.11.2",
    "pinia": "2.0.14",
    "vue": "3.2.36"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@ttou/define-config": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "h3": "^0.7.8",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.2",
    "nuxt": "^3.0.0-rc.3",
    "postcss": "^8.4.8",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.0",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.7.2",
    "ufo": "^0.8.3",
    "vite": "^2.9.9"
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:script",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
