{
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[vue]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "eslint.enable": false,
  "prettier.enable": false,
  "path-intellisense.mappings": {
    "@": "${workspaceFolder}/src"
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelint.enable": true,
  "vue.server.hybridMode": false,
  "cSpell.words": [
    "biomejs",
    "commitlint",
    "demi",
    "iconify",
    "nuxi",
    "Nuxt",
    "nuxtjs",
    "pinia",
    "preinstall",
    "stylelint",
    "typecheck",
    "unplugin",
    "vueuse"
  ]
}
