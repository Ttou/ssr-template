{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "typescript-plugin-css-modules",
        "options": {
          "classnameTransform": "camelCaseOnly",
          "customMatcher": "\\.module\\.css$",
        }
      }
    ],
    "types": ["@pinia/nuxt", "element-plus/global", "./types"]
  }
}
